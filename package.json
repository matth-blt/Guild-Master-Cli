{
  "name": "guild-master-cli",
  "version": "1.0.0",
  "description": "Guild Master - A guild management RPG game in CLI",
  "type": "module",
  "main": "index.js",
  "bin": {
    "guild-master": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "build:mac": "npx caxa --input . --output dist/guild-master --exclude dist --exclude .git --exclude saves -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/index.js\"",
    "build:win": "npx caxa --input . --output dist/guild-master.exe --exclude dist --exclude .git --exclude saves -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/index.js\""
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "figlet": "^1.8.0",
    "gradient-string": "^3.0.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "caxa": "^3.0.1"
  },
  "keywords": [
    "cli",
    "game",
    "guild",
    "rpg",
    "terminal"
  ],
  "author": "matth-blt",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}